  aliasql    String      @db.VarChar(32)
  oraschema  String      @db.Char(8)
  descr      String      @db.VarChar(50)
  orarelease String?     @db.VarChar(32)
  typenvid   Int?
  psadm_env  psadm_env[]

  @@id([oracle_sid, aliasql, oraschema])
  @@index([aliasql])
}

model psadm_param {
  param  String @id @db.VarChar(12)
  valeur String @db.VarChar(100)
